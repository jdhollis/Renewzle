<h2>Discount</h2>

<dl>
  <dd>Company: <%= @discount.company %></dd>
  <dd>Price: $<%= number_with_precision(@discount.price) %></dd>
  <dd>Percentage: <%= @discount.percentage %>%</dd>
  <dd>First Lead: <%= (@discount.type == FirstLeadDiscount) ? 'Yes' : 'No' %></dd>
</dl>

<%= link_to 'Edit', edit_admin_discount_path(@discount) %> | <%= link_to 'Back', admin_discounts_path %>
