<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
  <!-- <%= server_name %> -->
  <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
  
  <title><%= @view_title.blank? ? 'Renewzle' : "Renewzle: #{@view_title}" %></title>
  
  <%= stylesheet_link_tag('admin') %>
  
  <%= javascript_include_tag(:defaults) %>
</head>

<body>
  <div id="header">
    <ul id="navi">
      <li><%= link_to('Dashboard', admin_dashboard_url) %></li>
      <li style="margin-right:3em"><%= link_to('Masquerade', masquerade_panel_url) %></li>
      
      <li><%= link_to('Backgrounders', admin_backgrounders_url) %></li>
      <li><%= link_to('Companies', admin_companies_url) %></li>
      <li><%= link_to('Discounts', admin_discounts_url) %></li>
      <li><%= link_to('Incentives', admin_incentives_url) %></li>
      <li><%= link_to('Leads', admin_leads_url) %></li>
      <li><%= link_to('Profiles', admin_profiles_url) %></li>
      <li><%= link_to('Quotes', admin_quotes_url) %></li>
      <li><%= link_to('Users', admin_users_url) %></li>
      
      
      <li style="float:right;"><%= link_to('Logout', logout_url) %></li>
    </ul>
  </div>
  
  <div id="messages">
		<%= message_from(flash) %>
  </div>

  <div id="content">
    <%= yield %>
  </div>
  
  <div id="footer">
    <span></span>
  </div>

<%= page_javascript %>
</body>
</html>
